{
    "name": "anime-art-forwarder",
    "version": "0.1.0",
    "description": "A simple and efficient Chrome browser extension that streamlines the process of sending anime art to your Telegram channel through a Telegram bot. Quickly forward anime art from various sources directly to your Telegram channel with just one click.",
    "private": true,
    "scripts": {
        "watch": "webpack --mode=development --watch --config config/webpack.config.js",
        "build": "webpack --mode=production --config config/webpack.config.js",
        "pack": "node pack.js",
        "repack": "npm run build && npm run pack",
        "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\"",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/preset-env": "^7.22.2",
        "adm-zip": "^0.5.10",
        "babel-jest": "^29.5.0",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.7.3",
        "file-loader": "^6.2.0",
        "jest": "^29.5.0",
        "mini-css-extract-plugin": "^2.7.5",
        "prettier": "^2.8.8",
        "sass": "^1.62.1",
        "sass-loader": "^13.2.2",
        "style-loader": "^3.3.2",
        "vue": "^3.2.47",
        "vue-loader": "^17.1.0",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.7.14",
        "webpack": "^5.81.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0"
    }
}
